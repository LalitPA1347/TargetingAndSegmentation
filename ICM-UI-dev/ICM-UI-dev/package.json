{
  "name": "icm-tool-ui",
  "version": "1.0.0",
  "description": "React app with Typescript",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.dev.js",
    "build:run": "serve -s dist",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:fix": "npm run lint:code:fix && npm run lint:style:fix",
    "lint:code": "eslint src/**/*.{js,ts,tsx} --quiet",
    "lint:code:fix": "eslint src/**/*.{js,ts,tsx} --fix",
    "lint:style": "stylelint src/**/*.scss",
    "lint:style:fix": "stylelint src/**/*.scss --fix",
    "test:jest": "jest --config=jest.config.js",
    "test:jest:watch": "jest --config=jest.config.js --watch",
    "test:jest:coverage": "jest --config=jest.config.js --coverage",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open"
  },
  "hooks": {
    "pre-commit": "npm run lint && npm run test:jest"
  },
  "keywords": [
    "react",
    "typescript",
    "javascript",
    "jest"
  ],
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/parser": "^7.18.0",
    "css-loader": "^7.1.2",
    "cypress": "^13.13.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "file-loader": "^6.2.0",
    "globals": "^15.8.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.8.1",
    "stylelint-config-sass-guidelines": "^12.0.0",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/base": "^5.0.0-beta.60",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "@tanstack/react-query": "^5.59.15",
    "@toolpad/core": "^0.8.1",
    "antd": "^5.21.5",
    "babel-preset-es2015": "^6.24.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-router-dom": "^6.25.1",
    "react-test-renderer": "^18.3.1",
    "zod": "^3.23.8",
    "react-toastify": "^10.0.6"
  }
}
